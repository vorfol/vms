{
    "name": "vms",
    "displayName": "VMS",
    "description": "",
    "version": "0.0.1",
    "publisher": "vorfol",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:VMS.buildProject",
        "onCommand:VMS.debugProject"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "VMS.buildProject",
                "title": "Build on VMS"
            },
            {
                "command": "VMS.debugProject",
                "title": "Debug on VMS"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "ssh2": "^0.6.1",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.18"
    },
    "dependencies": {
        "@types/ssh2": "^0.5.35"
    }
}
